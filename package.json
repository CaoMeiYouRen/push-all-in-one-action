  "private": true,
  "description": "基于 push-all-in-one 的 action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "lint": "cross-env NODE_ENV=production eslint src __tests__ *.js --fix --ext .ts,.js",
