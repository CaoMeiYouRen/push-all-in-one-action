{
  "name": "push-all-in-one-action",
  "version": "1.0.3",
  "private": true,
  "description": "基于 push-all-in-one 的定时推送 action",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "dev": "cross-env NODE_ENV=development ts-node-dev src/index.ts",
    "build": "cross-env NODE_ENV=production rollup -c",
    "lint": "cross-env NODE_ENV=production eslint src __tests__ *.js --fix --ext .ts,.js",
    "format": "npm run lint",
    "format-check": "npm run lint",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "release": "semantic-release",
    "all": "npm run build && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CaoMeiYouRen/push-all-in-one-action.git"
  },
  "keywords": [
    "schedule",
    "notice",
    "actions",
    "node",
    "push",
    "server-chan",
    "serverchan",
    "server-chan-turbo",
    "CoolPush",
    "email",
    "dingtalk",
    "weixin",
    "wechat",
    "pushplus",
    "push+",
    "iGot"
  ],
  "author": "CaoMeiYouRen",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "axios": "^0.21.1",
    "colors": "^1.4.0",
    "push-all-in-one": "^1.3.4"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.168",
    "@types/node": "^15.0.1",
    "@typescript-eslint/parser": "^4.8.1",
    "@vercel/ncc": "^0.27.0",
    "conventional-changelog-cmyr-config": "^1.2.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.17.0",
    "eslint-config-cmyr": "^1.1.7",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "js-yaml": "^4.0.0",
    "lodash": "^4.17.21",
    "rimraf": "^3.0.2",
    "rollup": "^2.40.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "semantic-release": "^17.4.0",
    "ts-jest": "^26.5.2",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.1.3"
  }
}
